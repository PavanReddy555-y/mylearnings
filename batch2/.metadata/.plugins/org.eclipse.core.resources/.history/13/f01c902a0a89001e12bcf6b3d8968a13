package com.cn.set1;

public class Test  {
	       
	
	    public static void main(String[] args) {
			B bq = new B();
			bq.access();
		}
	
}          
class A
{
 int a=10;
}
 
class B extends A
{
	int b;
 public B() {
	this.b = super.a;
	System.out.println(b);
 }
 
 public  void  access() {
	 System.out.println(super.a);
 }
}
 
class C extends B
{
 
}
 
